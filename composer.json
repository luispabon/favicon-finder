{
  "name": "luispabon/favicon-finder",
  "type": "library",
  "description": "PHP Library used to discover favicon from given URL",
  "keywords": [
    "favicon",
    "icon",
    "finder"
  ],
  "homepage": "https://github.com/luispabon/favicon",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Chris Shiflett",
      "homepage": "http://shiflett.org/"
    },
    {
      "name": "Arthur Hoaro",
      "homepage": "http://hoa.ro"
    },
    {
      "name": "Luis Pabon",
      "homepage": "https://github.com/luispabon"
    }
  ],
  "support": {
    "issues": "https://github.com/luispabon/favicon-finder/issues",
    "sources": "https://github.com/luispabon/favicon-finder"
  },
  "require": {
    "php":               ">=7.3.0",
    "ext-dom":           "*",
    "psr/simple-cache":  "^1.0.1",
    "guzzlehttp/guzzle": "^6.4"
  },
  "require-dev": {
    "symfony/var-dumper": "^4.3",
    "phpunit/phpunit": "9.3.*",
    "guzzlehttp/psr7": "^1.6",
    "infection/infection": "^0.18",
    "phpstan/phpstan": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "FaviconFinder\\": "src/"
    }
  }
}
